<h1 class="text-center mt-4">Lista de Productos</h1>
<div class="container">
    <ul class="list-group">
        {{#each products}}
        <li class="list-group-item d-flex justify-content-between align-items-center">
            {{this.title}} - ${{this.price}}
        </li>
        {{/each}}
    </ul>
</div>

